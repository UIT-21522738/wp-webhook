<h1 class="my-4">WooCommerce Products</h1>
    <div class="row">
        {{#each products}}
            <div class="col-md-4">
                <div class="card mb-4 shadow-sm">
                    {{!-- <img src="{{images.[0].src}}" class="card-img-top" alt="{{name}}"> --}}
                    <div class="card-body">
                        <img src="{{images.[0].src}}" alt="">
                        <h5 class="card-title">{{name}}</h5>
                        <p class="card-text">{{price}}</p>
                        {{{description}}}
                    </div>
                </div>
            </div>
        {{/each}}
    </div>
</div>